{
  "name": "book-store-server",
  "version": "1.0.0",
  "description": "This is a book store project where user can sell or rent and buy book",
  "main": "app.js",
  "scripts": {
    "start": "tsc && prisma generate && node dist/app.js",
    "dev": "nodemon dist/app.js",
    "compile": "tsc --watch",
    "build": "prisma generate && rimraf dist && tsc",
    "ts.check": "tsc --project tsconfig.json",
    "add-build": "git add dist",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "pre-commit": [
    "ts.check",
    "build",
    "add-build"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wdmahbubur/book-store.git"
  },
  "keywords": [
    "book-store",
    "store",
    "book",
    "sell",
    "rent",
    "buy"
  ],
  "author": "Md Mahbubur Rahman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wdmahbubur/book-store/issues"
  },
  "homepage": "https://github.com/wdmahbubur/book-store#readme",
  "dependencies": {
    "@apollo/server": "^4.9.5",
    "@prisma/client": "^5.5.2",
    "@types/jsonwebtoken": "^9.0.5",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "graphql": "^16.8.1",
    "jsonwebtoken": "^9.0.2",
    "postmark": "^3.1.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.16",
    "@types/express": "^4.17.21",
    "@types/node": "^20.9.0",
    "nodemon": "^3.0.1",
    "pre-commit": "^1.2.2",
    "prisma": "^5.5.2",
    "rimraf": "^5.0.5",
    "typescript": "^5.2.2"
  }
}